<div class="grid_index">
  <% @events.each do |event|%>
  <div class="grid_index_data">
    <%= link_to event_path(event) do %>
      <%= image_tag "/assets/#{event.image_url}", class: 'event_image' %>
      <p><j2><%= event.name %></j2></p>
    <% end %>
  </div>
  <%end%>
</div>
