



<h3>LogIn</h3>



<% if flash[:message] %>
<%= flash[:message] %>
<% end %>


<%= form_tag login_path do %>
<%= label_tag :name %>
<%= text_field_tag :name %><br><br>
<%= label_tag :password %>
<%= text_field_tag :password %><br><br>
<%= submit_tag 'Log in' %>
<% end %>

<%= link_to 'Back', '/' %>
